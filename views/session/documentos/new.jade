				script.
					$(document).ready(function(){
						/* Guarda la selecci칩n de las formas */
						let selected = [];

						let selectLimit = 3;

						let card = {
							element: $("#toggableCard"),
							title: $(".card-title"),
							text: $(".card-text")
						};

						let selectedZones = {
							element: $("#selectedZones"),
							badge: $(".badge"),
							zones: $(".body-zones")
						};

						/* Eventos sobre los grupos de formas */

						$("g").on("mouseenter", function(event){
							if ($(this).css("opacity") == 0) {
								$(this).css({"opacity":0.3})
							}

							openCard(event);
						});

						$("g").on("mousemove", function(event){
							openCard(event);
						});

						$("g").on("mouseleave", function(event){
							if($(this).css("opacity") == 0.3) {
								$(this).css({"opacity":0});
							}

							closeCard();
						});

						$("g").on("click", function(event){
							let indexSelected;

							if($(this).css("opacity") == 0.3) {
								$(this).css({"opacity":0.6});
								selected.push(event.currentTarget);
							} else if ($(this).css("opacity") == 0.6) {
								$(this).css({"opacity":0.3});
								indexSelected = selected.findIndex(function(element){
									return $(element).is(event.currentTarget);
								});
								selected = selected.slice(0, indexSelected).concat(selected.slice(indexSelected + 1));
							}

							editCard(event);
							setZones();
						});

						/* Card */

						function openCard(event){
							let posX = event.clientX;
							let posY = event.clientY;
							editCard(event);
							card.element.css({
								"top": (posY + 20) + "px",
								"left": (posX + 20) + "px",
								"display": "block"
							});
						}

						function editCard(event){
							let title = $(event.currentTarget).attr("title");
							card.title.html(title);
							if (isSelected(event)){
								card.text.html("<p><i class='fa fa-plus fa-fw'></i> Selecciona para agregar</p>");
							} else {
								card.text.html("<p class='text-success'><i class='fa fa-check'></i> Agregada</p>");
							}
						}

						function closeCard(){
							card.element.css({
								"display":"none"
							});
						}

						/* Body Zones */

						function setZones(){
							let zonesStr = "";
							let currentSelect = 0;

							selectedZones.badge.html(selected.length);
							selectedZones.zones.html("");

							for (let i = 0; i < selected.length; i++) {
								currentSelect++;

								let d = document.createElement("div");
								$(d).addClass("alert")
									.addClass((currentSelect > selectLimit) ? "alert-danger" : "alert-primary")
									.addClass("fade")
									.addClass("show")
									.attr("role", "alert")
									.html($(selected[i]).attr("title"));

								let b = document.createElement("button");
								$(b).addClass("close")
									.attr("data-dismiss", "alert")
									.attr("aria-label", "Close")
									.html("&times")
									.on("click",
									function(){
										deselectElement(selected[i]);
									}
								);

								$(b).appendTo($(d));
								$(d).appendTo($(".body-zones"));

								if (currentSelect > selectLimit){
									selectedZones.badge.addClass("badge-danger").removeClass("badge-primary");
								} else {
									selectedZones.badge.addClass("badge-primary").removeClass("badge-danger");
								}
							}

							selectedZones.element.css({
								"display": (selected.length != 0 ? "block": "none")
							});
						};

						/* Auxiliar Functions */

						function deselectElement(elementToDelete){
							let index =  selected.findIndex(function(element){
								return $(element).is(elementToDelete);
							});
							selected = selected.slice(0, index).concat(selected.slice(index + 1));
							$(elementToDelete).css({"opacity":"0"}); 
							setZones();

						};

						function isSelected(e) {
							let index = selected.findIndex(function(element){
								return $(element).is(event.currentTarget);
							});
							return (index == -1);
						}
					});

